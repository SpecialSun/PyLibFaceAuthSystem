    border-radius: 6px; /* 圆角 */  

}  
  
/* 设置表头的样式 */  
QTableWidget#tableWidget_student QHeaderView::section {  
    background-color: #f0f0f0; /* 浅灰色背景 */  
    font-family: &quot;Arial&quot;;  
    font-size: 20px;  
    font-weight: bold;  
    color: #333; /* 文本颜色 */  
    border: 1px solid #ccc; /* 边框 */  
    border-right: none; /* 去除右边的边框，使得相邻的 section 边框连续 */  
    padding: 4px; /* 内边距 */  
    text-align: center; /* 文本对齐方式 */  
    text-transform: uppercase; /* 文本大写 */  
}  
  
/* 设置表头的鼠标悬停效果 */  
QTableWidget#tableWidget_student QHeaderView::section:hover {  
    background-color: #e0e0e0; /* 鼠标悬停时的背景色 */  
}  
  
/* 设置表格单元格的样式 */  
QTableWidget#tableWidget_student QTableWidgetItem {  
    border-bottom: 1px solid #ccc; /* 单元格底部边框 */  
    padding: 4px; /* 内边距 */  
}  
  
/* 设置选中行的样式 */  
QTableWidget#tableWidget_student::item:selected {  
    background-color: #a0a0a0; /* 选中时的背景色 */  
    color: #ffffff; /* 选中时的文本颜色 */  
}  
  
/* 设置鼠标悬停在单元格上的样式 */  
QTableWidget#tableWidget_student::item:hover {  
    background-color: #e0e0e0; /* 鼠标悬停时的背景色 */  
}  
  
/* 去除最后一行的底部边框，如果需要的话 */  
QTableWidget#tableWidget_student QTableWidgetItem:last-child {  
    border-bottom: none;  
}</string>
